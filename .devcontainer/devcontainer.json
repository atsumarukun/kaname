{
  "dockerComposeFile": ["../docker-compose.yml"],
  "service": "api",
  "workspaceFolder": "/go/src/backend",
  "postAttachCommand": "git config --global --add safe.directory /go/src",
  "customizations": {
    "vscode": {
      "settings": {
        "git.openRepositoryInParentFolders": "always"
      },
      "extensions": [
        "golang.Go",
        "shardulm94.trailing-spaces"
      ]
    }
  }
}